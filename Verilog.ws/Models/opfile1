#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
S_0x563d1b0b1e10 .scope module, "tb_traffic_controller" "tb_traffic_controller" 2 1;
 .timescale 0 0;
v0x563d1b0c4ef0_0 .var "cur_state", 0 0;
v0x563d1b0c4f90_0 .var/i "i", 31 0;
v0x563d1b0c5050_0 .net "next_state", 2 0, v0x563d1b0c4cd0_0;  1 drivers
v0x563d1b0c5150_0 .var "prev_state", 0 0;
S_0x563d1b0b1fa0 .scope module, "rm1" "traffic_controller" 2 10, 3 1 0, S_0x563d1b0b1e10;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "cur_state";
    .port_info 1 /INPUT 1 "prev_state";
    .port_info 2 /OUTPUT 3 "next_state";
v0x563d1b0b21f0_0 .net "cur_state", 0 0, v0x563d1b0c4ef0_0;  1 drivers
v0x563d1b0c4cd0_0 .var "next_state", 2 0;
v0x563d1b0c4dd0_0 .net "prev_state", 0 0, v0x563d1b0c5150_0;  1 drivers
E_0x563d1b0b0430 .event anyedge, v0x563d1b0c4dd0_0, v0x563d1b0b21f0_0;
    .scope S_0x563d1b0b1fa0;
T_0 ;
    %wait E_0x563d1b0b0430;
    %load/vec4 v0x563d1b0b21f0_0;
    %load/vec4 v0x563d1b0c4dd0_0;
    %concat/vec4; draw_concat_vec4
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x563d1b0c4cd0_0, 0;
    %jmp T_0.5;
T_0.0 ;
    %pushi/vec4 1, 0, 3;
    %assign/vec4 v0x563d1b0c4cd0_0, 0;
    %jmp T_0.5;
T_0.1 ;
    %pushi/vec4 2, 0, 3;
    %assign/vec4 v0x563d1b0c4cd0_0, 0;
    %jmp T_0.5;
T_0.2 ;
    %pushi/vec4 3, 0, 3;
    %assign/vec4 v0x563d1b0c4cd0_0, 0;
    %jmp T_0.5;
T_0.3 ;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x563d1b0c4cd0_0, 0;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x563d1b0b1e10;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x563d1b0c4ef0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x563d1b0c5150_0, 0;
    %vpi_call 2 19 "$dumpfile", "tb_traffic_controller.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x563d1b0b1e10 {0 0 0};
    %vpi_call 2 21 "$monitor", "cur_state=0x%0h prev_state=0x%0h next_state=0x%0h", v0x563d1b0c4ef0_0, v0x563d1b0c5150_0, v0x563d1b0c5050_0 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x563d1b0c4f90_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x563d1b0c4f90_0;
    %cmpi/s 5, 0, 32;
    %jmp/0xz T_1.1, 5;
    %delay 5, 0;
    %vpi_func 2 25 "$random" 32 {0 0 0};
    %pad/s 1;
    %assign/vec4 v0x563d1b0c4ef0_0, 0;
    %vpi_func 2 26 "$random" 32 {0 0 0};
    %pad/s 1;
    %assign/vec4 v0x563d1b0c5150_0, 0;
    %load/vec4 v0x563d1b0c4f90_0;
    %addi 1, 0, 32;
    %store/vec4 v0x563d1b0c4f90_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
