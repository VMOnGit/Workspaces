v0x55d7a03faea0_0 .var "LOCK", 0 0;
v0x55d7a03faf70_0 .var "PIN", 0 0;
v0x55d7a03fb040_0 .var/i "i", 31 0;
S_0x55d7a03e6500 .scope module, "rm1" "security_test" 2 11, 3 1 0, S_0x55d7a03e6370;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "KC";
    .port_info 1 /INPUT 1 "PIN";
    .port_info 2 /INPUT 1 "LOCK";
    .port_info 3 /OUTPUT 1 "AL";
v0x55d7a03e6750_0 .var "AL", 0 0;
v0x55d7a03faaa0_0 .net "KC", 0 0, v0x55d7a03fae00_0;  1 drivers
v0x55d7a03fab60_0 .net "LOCK", 0 0, v0x55d7a03faea0_0;  1 drivers
v0x55d7a03fac00_0 .net "PIN", 0 0, v0x55d7a03faf70_0;  1 drivers
E_0x55d7a03e69f0 .event anyedge, v0x55d7a03fab60_0, v0x55d7a03fac00_0, v0x55d7a03faaa0_0;
    .scope S_0x55d7a03e6500;
T_0 ;
    %vpi_call 3 9 "$monitor", "KC=%b PIN=%b LOCK=%b AL=%b", v0x55d7a03faaa0_0, v0x55d7a03fac00_0, v0x55d7a03fab60_0, v0x55d7a03e6750_0 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x55d7a03e6500;
T_1 ;
    %wait E_0x55d7a03e69f0;
    %load/vec4 v0x55d7a03faaa0_0;
    %load/vec4 v0x55d7a03fab60_0;
    %concat/vec4; draw_concat_vec4
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_1.3, 6;
    %jmp T_1.4;
T_1.0 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55d7a03e6750_0, 0;
    %jmp T_1.4;
T_1.1 ;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55d7a03e6750_0, 0;
    %jmp T_1.4;
T_1.2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55d7a03e6750_0, 0;
    %jmp T_1.4;
T_1.3 ;
    %load/vec4 v0x55d7a03fac00_0;
    %nor/r;
    %assign/vec4 v0x55d7a03e6750_0, 0;
    %jmp T_1.4;
T_1.4 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x55d7a03e6370;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55d7a03fae00_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55d7a03faf70_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55d7a03faea0_0, 0;
    %vpi_call 2 22 "$dumpfile", "tb_security_test.vcd" {0 0 0};
    %vpi_call 2 23 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55d7a03e6370 {0 0 0};
    %vpi_call 2 24 "$monitor", "KC=0x%0h PIN=0x%0h LOCK=0x%0h AL=0x%0h", v0x55d7a03fae00_0, v0x55d7a03faf70_0, v0x55d7a03faea0_0, v0x55d7a03fad40_0 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55d7a03fb040_0, 0, 32;
T_2.0 ;
    %load/vec4 v0x55d7a03fb040_0;
    %cmpi/s 5, 0, 32;
    %jmp/0xz T_2.1, 5;
    %delay 5, 0;
    %vpi_func 2 28 "$random" 32 {0 0 0};
    %pad/s 1;
    %assign/vec4 v0x55d7a03fae00_0, 0;
    %vpi_func 2 29 "$random" 32 {0 0 0};
    %pad/s 1;
    %assign/vec4 v0x55d7a03faf70_0, 0;
    %vpi_func 2 30 "$random" 32 {0 0 0};
    %pad/s 1;
    %assign/vec4 v0x55d7a03faea0_0, 0;
    %load/vec4 v0x55d7a03fb040_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55d7a03fb040_0, 0, 32;
    %jmp T_2.0;
T_2.1 ;
    %end;
    .thread T_2;
